msgstr "Előző"

#: includes/class-sensei-course-progress-widget.php:211
msgid "Next"
msgstr "Következő"

#: includes/class-sensei-course-progress-widget.php:251
msgid "Other Lessons"
msgstr "További Leckék"

#: includes/class-sensei-course-progress-widget.php:319
msgid "Display all Modules"
msgstr "Összes Modul megjelenítése"

#: includes/class-sensei-course-progress-widget.php:322
